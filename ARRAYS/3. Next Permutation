import java.util.*;
class Solution {
    public void nextPermutation(int[] nums) {
        int ind =-1;
        int n =nums.length;
        for(int i=n-2;i>=0;i--){
            if(nums[i]<nums[i+1]){
                 ind =i;
                break;
            }
        }
    
        if(ind ==-1){
           reverseArray(nums,0,n-1);
           System.out.print(Arrays.toString(nums));
           return;
        }
        for(int i=n-1;i>ind;i--){
            if(nums[i]>nums[ind]){
                int tmp=nums[i];
                nums[i]=nums[ind];
                nums[ind]=tmp;
                break;

            }
        }
       reverseArray(nums,ind+1,n-1);
       System.out.print(Arrays.toString(nums));
    }
    private void reverseArray(int[] nums, int start,int end){
        while(start<end){
            int temp=nums[start];
            nums[start]=nums[end];
            nums[end]=temp;
            start++;
            end--;
        }
    }
}
